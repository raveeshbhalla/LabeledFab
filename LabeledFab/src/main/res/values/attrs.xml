<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabeledFab">
        <attr name="labelText" format="string"/>
        <attr name="labelMargin" format="dimension"/>
        <attr name="labelPadding" format="dimension"/>
        <attr name="labelIcon" format="reference"/>
        <attr name="backgroundColor" format="reference"/>
        <attr name="tooltipSide" format="enum">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>
</resources>